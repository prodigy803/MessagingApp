<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.joshi.whatsappclone1.CheckOutFragment"
    android:id="@+id/RelativeLayout1">

    <!-- TODO: Update blank fragment layout -->
    <Button
        android:text="LogOut?"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/goToLogoutFromMainActivityButton" />
    <Button
        android:text="Delete Account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:id="@+id/removeUser"
        android:layout_below="@+id/goToLogoutFromMainActivityButton"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:textSize="40dp"
        android:textStyle="bold"
        android:text="CheckOut"
        android:id="@+id/FragOneText"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="83dp" />


</RelativeLayout>